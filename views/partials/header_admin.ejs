    <nav class="header__nav">
        <ul>
            <li>
                <a href="/" class="">Home</a>
            </li>
            <li>
                <a href="/about" class="">About</a>
            </li>
            <li>
                <a href="/contact" class="">Contact</a>
            </li>
            <% if (isWebMaster) { %>
                <li>
                    <a href="/admin/webmaster" class="">Webmaster</a>
